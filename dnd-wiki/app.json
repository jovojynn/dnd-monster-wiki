{
  "expo": {
    "name": "dnd-wiki",
    "slug": "dnd-wiki",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/dnd-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/android-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#28282B"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/dnd-logo.png",
      "supportsTablet": true
    },
    "android": {
      "icon": "./assets/dnd-logo.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/dnd-nameonly.png",
        "backgroundColor": "#28282B"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
